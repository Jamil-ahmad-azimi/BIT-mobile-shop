        </tr>
          <% if (wholesalers && wholesalers.length === 0) { %>
  <tr><td colspan="10" class="text-center">End of the List</td></tr>
<% } else { %>
  <% wholesalers.forEach(wholesaler => { %>
    <tr>
      <td><%= wholesaler.wholesalerId %></td>
      <td><%= wholesaler.companyName %></td>
      <td><%= wholesaler.contactPerson %></td>
      <td><%= wholesaler.phoneNumber %></td>
      <td><%= wholesaler.email %></td>
      <td><%= wholesaler.address %></td>
      <td><%= wholesaler.productsSupplied %></td>
      <td><%= wholesaler.balance %></td>
      <td><%= wholesaler.status %></td>
      <td><%= wholesaler.notes %></td>
    </tr>
  <% }); %>
<% } %>

      </tbody>
    </table>
  </div>
</div>
